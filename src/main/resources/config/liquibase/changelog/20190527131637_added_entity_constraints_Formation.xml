<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Formation.
    -->
    <changeSet id="20190527131637-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="formation_id"
                                 baseTableName="formation_cours"
                                 constraintName="fk_formation_cours_formation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="formation"/>

        <addForeignKeyConstraint baseColumnNames="cours_id"
                                 baseTableName="formation_cours"
                                 constraintName="fk_formation_cours_cours_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cours"/>
        
    </changeSet>
</databaseChangeLog>
